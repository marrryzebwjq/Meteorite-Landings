SUR NOTRE DATASET :

QUERY 1

PREFIX schema: <https://schema.org/>
PREFIX ex: <https://example.org/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT ?name (STR(?weight) AS ?mass)
WHERE {
       ?meteorite schema:name ?name ;
                          schema:weight ?weight .
}
ORDER BY DESC(?weight)
LIMIT 10

