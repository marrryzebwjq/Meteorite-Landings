QUERY 2

PREFIX schema: <https://schema.org/>
PREFIX ex: <https://example.org/>

SELECT ?name (STR(?weight) AS ?mass)
WHERE {
       ?meteorite schema:year ?year ;
                           schema:name ?name ;
                           schema:weight ?weight .
       FILTER(STR(?year) = "2003.0")
}
ORDER BY DESC(?weight)
LIMIT 1

